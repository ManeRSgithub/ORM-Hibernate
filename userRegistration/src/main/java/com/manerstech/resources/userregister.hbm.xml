<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.manerstech.entities.userRegister" table="user_Registration_Details">
    		<id  name="id" column="user_id" />
    		<property name="name" column="user_name" />
    		<property name="email" column="user_email" />
    		<property name="contactNo" column="user_contactNO" />
    		<property name="password" column="user_password" />
    	
    	</class>
    
    </hibernate-mapping>